{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "baseUrl": "./resources/js",
        "paths": {
            "@/*": ["./*"]
        },
        "types": ["vite/client"]
    },
    "include": ["resources/js/**/*"]
}
